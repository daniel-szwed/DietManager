<Application x:Class="ArbreSoft.DietManager.Presentation.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ArbreSoft.DietManager.Presentation" 
             xmlns:converters="clr-namespace:ArbreSoft.DietManager.Presentation.Converters">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary 
                    x:Name="IconsDictionary" 
                    Source="pack://application:,,,/ArbreSoft.DietManager.Presentation;component/Resources/Icons.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
